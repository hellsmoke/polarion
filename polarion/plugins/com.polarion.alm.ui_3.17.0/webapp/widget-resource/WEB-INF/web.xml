<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN"
    "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">

<web-app metadata-complete="true">
	<display-name>Widget Resources</display-name>
	
    <servlet>
	    <servlet-name>WidgetResourcesServlet</servlet-name>
	    <servlet-class>com.polarion.alm.server.api.model.rp.widget.WidgetResourcesServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>WidgetResourcesServlet</servlet-name>
        <url-pattern>/*</url-pattern>
    </servlet-mapping>
	
	<session-config>
		<session-timeout>30</session-timeout>
	</session-config>
	
</web-app>
