<content>
#if($xwiki.hasAdminRights())
  #panelheader1("Spaces")
  &lt;div class="spacepanel"&gt;

  ##set($spaces = $xwiki.getCurrentSpaces())
  #if ($doc.getParentForPanel())
    #set($spaces = $xwiki.getCurrentSpaces("$doc.getParentForPanel()"))
  #else
    #set($spaces = $xwiki.getCurrentSpaces())
  #end
  #set($url = $xwiki.getRequestURL())
  #set( $restOfLinks = false )
  #set($titleindex = $msg.get("listofpages"))
  #foreach($space in $spaces)
    #set($space = $xwiki.format($space))
    #set($title = $xwiki.getPageTitle($space))
    #if( $space.startsWith("/") )
 		#set( $repoLink = true ) 
    #end
    #if( $repoLink )
	#if( !$restOfLinks )		
		#if( !$space.startsWith("/") )
			----
			#set( $restOfLinks = true )
		#end	
	#end 
	#end
    #set($spaceInfo = $xwiki.getSpaceInfo($space))
    #set($spaceIndexURL = $xwiki.getSpaceIndexURL($space, $url))
    #set($spaceInteranlIndexURL = $xwiki.getInnerIndexURL($space, $url))
    &lt;span class="panelitem"&gt;&lt;a href="/polarion/wiki/bin/view/${space}/" title="$title" &gt;
    	$xwiki.getSpaceDisplayName($spaceInfo.name)
    &lt;/a&gt;       
     &lt;span class="underline" style="cursor: pointer;" onclick="window.parent.location.hash='$spaceIndexURL'; window.parent.working_area.location.replace('$spaceInteranlIndexURL');"&gt;(index)&lt;/span&gt;
    &lt;/span&gt;
  #end
  &lt;/div&gt;
  #panelfooter()
#end</content>
